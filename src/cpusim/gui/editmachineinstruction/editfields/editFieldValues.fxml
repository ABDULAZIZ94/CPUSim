<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>

<BorderPane fx:id="mainPane" prefHeight="453.0" prefWidth="473.0" styleClass="root"
            xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8">
    <bottom>
        <VBox id="VBox" alignment="CENTER" spacing="5.0">
            <HBox id="HBox" alignment="CENTER" prefHeight="36.0" prefWidth="473.0"
                  spacing="5.0">
                <Button id="new" mnemonicParsing="false" onAction="#handleNew"
                        prefHeight="22.0" styleClass="roundButton" text="    New    "/>
                <Button fx:id="delete" mnemonicParsing="false" onAction="#handleDelete"
                        prefHeight="22.0" styleClass="roundButton" text="   Delete   "/>
                <Button fx:id="duplicate" mnemonicParsing="false"
                        onAction="#handleDuplicate" prefHeight="22.0"
                        styleClass="roundButton" text="  Duplicate  "/>
            </HBox>
            <Separator prefWidth="473.0"/>
            <HBox id="HBox" alignment="CENTER" prefHeight="36.0" prefWidth="473.0"
                  spacing="5.0">
                <Button defaultButton="true" mnemonicParsing="false"
                        onAction="#handleOkay" prefHeight="22.0" prefWidth="160.0"
                        styleClass="roundBlueButton" text="OK"/>
                <Button cancelButton="true" mnemonicParsing="false"
                        onAction="#handleCancel" prefHeight="22.0" prefWidth="160.0"
                        styleClass="roundButton" text="Cancel"/>
            </HBox>
        </VBox>
    </bottom>
    <center>
        <TableView fx:id="table" editable="true" prefHeight="372.0" prefWidth="437.0">
            <columns>
                <TableColumn minWidth="0.0" prefWidth="75.0" text="Name" fx:id="name"/>
                <TableColumn minWidth="0.0" prefWidth="75.0" text="Value" fx:id="value"/>
            </columns>
        </TableView>
    </center>
    <stylesheets>
        <URL value="@../../css/Dialogs.css"/>
    </stylesheets>
</BorderPane>
