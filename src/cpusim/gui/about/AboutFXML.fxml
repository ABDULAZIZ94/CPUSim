<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.WebView?>
<?import java.net.URL?>

<BorderPane minWidth="450.0" prefHeight="700.0" prefWidth="450.0" styleClass="root"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
    <bottom>
        <HBox id="HBox" alignment="CENTER" prefHeight="49.0"
              prefWidth="450.0" spacing="5.0">
            <Button fx:id="okButton" contentDisplay="CENTER" defaultButton="true"
                    maxWidth="Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onAction="#onOkButtonClicked" prefHeight="22.0" prefWidth="160.0"
                    text="OK"/>
        </HBox>
    </bottom>
    <center>
        <WebView fx:id="webView" prefHeight="270.0" prefWidth="450.0"/>
    </center>
    <stylesheets>
        <URL value="@../css/Dialogs.css"/>
    </stylesheets>
    <top>
        <StackPane prefHeight="225.0" prefWidth="429.0">
            <ImageView fx:id="imageView" fitHeight="191.0" fitWidth="355.0"
                       pickOnBounds="true" preserveRatio="true"
                       />
        </StackPane>
    </top>
</BorderPane>
